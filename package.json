    "@babel/polyfill": "^7.4.4",
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-ws": "^1.0.17",
    "apollo-utilities": "^1.3.0",
    "bcryptjs": "^2.4.3",
    "cross-fetch": "^3.0.3",
    "env-cmd": "^9.0.1",
    "graphql": "^14.3.1",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "^1.17.4",
